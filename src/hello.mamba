package main;

type struct { x: int, y: int } Point;
type struct { pt1: Point, pt2: Point } Line;

def main() -> int {
  let line = newLine(2, 5, 6, 8);
  return line.pt2.y;
}

def newLine(x1: int, y1: int, x2: int, y2: int) -> Line {
  let pt1 = Point{x: x1, y: y1};
  let pt2 = Point{x: x2, y: y2};
  let line = Line{pt1: pt1, pt2: pt2};
  return line;
}
